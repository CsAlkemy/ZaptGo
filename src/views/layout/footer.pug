mixin footer
	#remoteModal1.modal.fade.modalToClose(role="dialog" data-backdrop="static" data-keyboard="false")
	#remoteModal2.modal.fade.modalToClose(role="dialog" style="z-index: 99999; background: rgba(1,1,1,.5);")

	script(src="/plugins/jquery/jquery.min.js")
	script(type="text/javascript" src="/plugins/popper/popper.min.js")
	script(src="/plugins/bootstrap/js/bootstrap.bundle.min.js")
	script(src="/plugins/toastr/toastr.min.js")
	script(src="/js/style.min.js")
	script(type="text/javascript" src="/plugins/moment/min/moment.min.js")
	script(type="text/javascript" src="/plugins/jquery-validation/jquery.validate.min.js")
	script(type="text/javascript" src="/plugins/daterangepicker/daterangepicker.js")
	script(src="//cdn.rawgit.com/saribe/eModal/1.2.67/dist/eModal.min.js")
	script(type="text/javascript" src="/js/scripts.js")
	each script in __pageScript
		each js in script.js
			script(src=js)